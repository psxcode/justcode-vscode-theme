{
  "name": "justcode-darcula",
  "type": "dark",
  "include" : "./_common.json",
  "tokenColors": [
    {
      "name": "jsx/tsx tags",
      "scope": [
        "support.class"
      ],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },
    {
      "name": "jsx/tsx tags html",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#ef596fff",
        "fontStyle": ""
      }
    },
    {
      "name": "jsx/tsx embedded braces",
      "scope": [
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#FFC66D",
        "fontStyle": ""
      }
    },
    {
      "name": "jsx/tsx attribute name",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": ""
      }
    },
    {
      "name": "html",
      "scope": [
        "text.html entity.name.tag",
        "text.html punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#FFC66D",
        "fontStyle": ""
      }
    },


    {
      "name": "constant numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#6897BB",
        "fontStyle": ""
      }
    },


    {
      "name": "js/ts storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#cc7832ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "js/ts keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#cc7832ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "js/ts keyword import/export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#cc7832ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "js/ts node module",
      "scope": [
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#cc7832ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "js/ts operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression"
      ],
      "settings": {
        "foreground": "#cc7832ff",
        "fontStyle": "bold"
      }
    },

    {
      "name": "js/ts function declaration",
      "scope": [
        "storage.type.function.arrow",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#cc7832ff",
        "fontStyle": "bold"
      }
    },


    {
      "name": "js/ts this/super",
      "scope": [
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#cc7832ff",
        "fontStyle": "bold"
      }
    },


    {
      "name": "js/ts caution",
      "scope": [
        "support.class.console",
        "variable.language.arguments",
        "keyword.other.debugger",
        "support.variable.dom",
        "keyword.operator.expression.delete"
      ],
      "settings": {
        "foreground": "#ef596fff",
        "fontStyle": ""
      }
    },
    {
      "name": "js/ts caution bold",
      "scope": [
        "meta.embedded.expression meta.objectliteral"
      ],
      "settings": {
        "foreground": "#ef596fff",
        "fontStyle": "bold"
      }
    },


    {
      "name": "js/ts function call",
      "scope": [
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#FFC66D",
        "fontStyle": ""
      }
    },
    {
      "name": "js/ts destructuring",
      "scope": [
        "punctuation.definition.binding-pattern"
      ],
      "settings": {
        "foreground": "#cc7832ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "js/ts builtin constants",
      "scope": [
        "constant.language",
        "support.constant",
        "support.class.builtin"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "js/ts other code",
      "scope": [
        "variable",
        "keyword.operator",
        "entity.other.inherited-class",
        "support.variable.property.process",
        "support.function.console",
        "meta.definition"
      ],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },


    {
      "name": "ts types",
      "scope": [
        "entity.name.type",
        "meta.object.type",
        "meta.type.annotation.ts punctuation.definition.parameters",
        "meta.type.annotation.ts storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#eee",
        "fontStyle": ""
      }
    },
    {
      "name": "ts types primitives",
      "scope": [
        "support.type.builtin",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#eee",
        "fontStyle": "italic"
      }
    },


    {
      "name": "js/ts punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#444",
        "fontStyle": ""
      }
    },
    {
      "name": "js/ts tuned down braces",
      "scope": [
        "meta.brace.round",
        "meta.block",
        "meta.class"
      ],
      "settings": {
        "foreground": "#444",
        "fontStyle": ""
      }
    },
    {
      "name": "jsx/tsx tuned down braces",
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": ""
      }
    },


    {
      "name": "js/ts property readwrite",
      "scope": [
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },
    {
      "name": "js/ts class declaration",
      "scope": [
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },


    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "#6A8759",
        "fontStyle": ""
      }
    },
    {
      "name": "js/ts template/string expression",
      "scope": [
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#98c378",
        "fontStyle": ""
      }
    },
    {
      "name": "js/ts template/string expression",
      "scope": [
        "punctuation.definition.template-expression",
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "foreground": "#6A8759",
        "fontStyle": ""
      }
    },


    {
      "name": "js/ts object key",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#9876AA",
        "fontStyle": ""
      }
    },
    {
      "name": "js/ts object creation",
      "scope": [
        "meta.interface punctuation.definition.block",
        "meta.objectliteral"
      ],
      "settings": {
        "foreground": "#9876AA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "js/ts object array creation",
      "scope": [
        "meta.array.literal"
      ],
      "settings": {
        "foreground": "#9876AA",
        "fontStyle": ""
      }
    },
    {
      "name": "js/ts object colon",
      "scope": [
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#444",
        "fontStyle": ""
      }
    },


    {
      "name": "js/ts method declaration",
      "scope": [
        "meta.method.declaration",
        "meta.definition.method"
      ],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },


    {
      "name": "js/ts tuned down properties chain",
      "scope": [
        "variable.other.object.property",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": ""
      }
    },


    {
      "name": "js/ts comment",
      "scope": [
        "punctuation.definition.comment",
        "comment.block.documentation storage.type.class.jsdoc",
        "comment.block.documentation entity.name.type.instance.jsdoc",
        "comment.block.documentation variable.other.jsdoc",
        "comment"
      ],
      "settings": {
        "foreground": "#464646",
        "fontStyle": ""
      }
    },


    {
      "name": "json punctuation",
      "scope": [
        "source.json punctuation",
        "source.json punctuation.definition.string",
        "source.json punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#444",
        "fontStyle": ""
      }
    },
    {
      "name": "json punctuation definition",
      "scope": [
        "source.json punctuation.definition"
      ],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },
    {
      "name": "json string",
      "scope": [
        "source.json string"
      ],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },
    {
      "name": "json constant",
      "scope": [
        "source.json constant.language",
        "source.json constant.numeric"
      ],
      "settings": {
        "foreground": "#6897BB",
        "fontStyle": ""
      }
    },
    {
      "name": "json keys",
      "scope": [
        "source.json support.type.property-name"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": ""
      }
    }
  ]
}